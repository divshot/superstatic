{
  "name": "superstatic",
  "version": "0.10.0",
  "description": "Superstatic: a static file server for fancy apps",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && node_modules/.bin/mocha --recursive --reporter spec ./test",
    "lint": "node_modules/.bin/jshint ./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divshot/superstatic.git"
  },
  "keywords": [
    "static",
    "server",
    "divshot",
    "hosting"
  ],
  "author": "Divshot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/divshot/superstatic/issues"
  },
  "dependencies": {
    "async": "0.4.x",
    "chalk": "~0.4.0",
    "chokidar": "~0.7.1",
    "connect": "2.9.x",
    "deliver": "~0.2.0",
    "drainer": "~0.2.1",
    "globject": "0.1.x",
    "is-url": "~1.0.0",
    "jsun": "0.1.x",
    "lodash": "2.4.x",
    "mime": "1.2.x",
    "minimatch": "0.2.x",
    "minimist": "0.0.8",
    "mix-into": "~0.1.1",
    "nash": "~0.4.0",
    "pathematics": "~0.1.1",
    "qmap": "~0.2.3",
    "slasher": "0.1.x"
  },
  "preferGlobal": "true",
  "bin": {
    "superstatic": "bin/server",
    "ss": "bin/server"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "clone": "0.1.x",
    "connect": "~2.9.2",
    "jshint": "2.4.x",
    "knox": "~0.8.6",
    "mkdirp": "0.3.x",
    "mocha": "~1.13.0",
    "mocksy": "~0.1.4",
    "pre-commit": "0.0.4",
    "request": "~2.34.0",
    "rmdir": "1.0.x",
    "sinon": "~1.7.3",
    "supertest": "~0.9.0",
    "through": "~2.3.4"
  },
  "jshintConfig": {
    "expr": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "sub": true,
    "undef": true,
    "unused": false,
    "boss": true,
    "eqnull": true,
    "node": true,
    "laxbreak": true,
    "globals": {
      "require": true,
      "module": true,
      "exports": true,
      "it": true,
      "describe": true,
      "beforeEach": true,
      "afterEach": true,
      "chai": true,
      "Regex": true
    }
  }
}
